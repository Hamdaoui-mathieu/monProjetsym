{% extends 'base.html.twig' %}

{% block body %}

<div class="fosy">
    <h1 class="titre">Contact</h1>
</div>
<div class="row text-center">

    {{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="row">
        <div class="col-9 mx-auto mb-2 mt-4">
            {{ form_row(form.objet) }}
        </div>
        <div class="col-9 mx-auto mb-2">
            {{ form_row(form.email) }}
        </div>
        <div class="col-9 mx-auto mb-2">
            {{ form_row(form.message) }}
        </div>

    </div>


    {{ form_end(form) }}
</div> 


{# <div>
    {{form(form)}}
</div> #}

 {% endblock %} 
